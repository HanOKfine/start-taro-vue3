// ESLint 检查 .vue 文件需要单独配置编辑器：
// https://eslint.vuejs.org/user-guide/#editor-integrations
{
  "extends": ["taro/vue3"],
  "parserOptions":{
    "parser": "@babel/eslint-parser",
    "requireConfigFile": false
  },
  "rules": {
    "vue/multi-word-component-names": "off"
  } ,
  "plugins": [
    "babel-plugin-import"
  ]
}
